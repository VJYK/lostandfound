<div class="row">
<div class="col-sm-6 col-sm-offset-3">
<h2 align="center">Report Found Item</h2>
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role= "button" aria-expanded="false">Sort by Category :   <span class="caret"></span></a>
        <ul class="dropdown-menu" role="menu">
            <% Category.all.each do |category|%>
            <li><%= link_to category.name, losts_path(category: category.name),class: "link"%></li>
            <%end%>
        </ul>
</li>
<% @founds.each do |found|%>
<div class="well">
    <small>Posted at : <%= found.created_at.strftime("%b %d. %Y") %></small>
    <small style="float:right"> Reported By : <%= found.user.username %></small>

    
	<h3>ItemName : <%= found.item_name %></h3>

	<p>Description : <%= found.item_desc %> </p>
	<p>Return at : <%= found.return %></p>
	
</div>
<%end%>	
</div>
</div>	